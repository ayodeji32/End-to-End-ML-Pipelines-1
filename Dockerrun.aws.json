{
   "AWSEBDockerrunVersion": "2",
   "containerDefinitions": [ 
      { 
        "name": "backend",
        "image": "ayodeji32/backend",
        "links": ["frontend"],
        "essential": false,
        "portMappings": [ 
            { 
               "containerPort": 8000,
               "hostPort": 8000
            }
         ],
         "memory": 1000
      },

      {
        "name": "frontend",
        "image": "ayodeji32/frontend",
        "essential": true,
        "portMappings": [ 
            { 
               "containerPort": 80,
               "hostPort": 80
            }
         ],
         "memory": 500
      }
   ] 
}